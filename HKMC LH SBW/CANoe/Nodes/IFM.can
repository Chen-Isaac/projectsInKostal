	const FAULT_MANAGER_MAXFAULTS 		= 94;
	int d_Task_Manager;
	int d_Task_Counter;	
  int d_Faults_Read;

	//Task management stuff
	const TASK_CLEAR_FAULT_MANAGER 		= 1;
	const TASK_READ_FAULT_MANAGER 		= 2;
