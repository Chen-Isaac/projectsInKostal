            <const>Press the lift up button and make locked roter situation happens in the top. Don't release the button in the course.</const>
          </value>
        </param>
        <param>
          <type>long</type>
          <value>
            <const>1</const>
          </value>
        </param>
      </canoestandardservice>
      <while>
        <title />
        <compare>
          <value>
            <dbobject>SysVar_BEGIN_OF_OBJECT|1|AvgCurrent|VTS::M12_Out1|-1|0|END_OF_OBJECT_SysVar|</dbobject>
          </value>
          <gt>
            <const>1</const>
          </gt>
        </compare>
        <comment>
          <title>AvgCurrent&gt;1 means the power window is working.</title>
        </comment>
        <if>
          <title />
          <compare>
            <value>
              <dbobject>SysVar_BEGIN_OF_OBJECT|1|AvgCurrent|VTS::M12_Out1|-1|0|END_OF_OBJECT_SysVar|</dbobject>
            </value>
            <gt>
              <const>14</const>
            </gt>
          </compare>
          <report>
            <text>Locked roter situation happened</text>
            <object>
              <dbobject>SysVar_BEGIN_OF_OBJECT|1|AvgCurrent|VTS::M12_Out1|-1|0|END_OF_OBJECT_SysVar|</dbobject>
            </object>
          </report>
          <break>
            <title />
          </break>
        </if>
        <wait>
          <title />
          <time>
            <value>
              <const>100</const>
            </value>
            <unit>ms</unit>
          </time>
        </wait>
      </while>
      <wait>
        <title />
        <time>
          <value>
            <const>1</const>
          </value>
          <unit>s</unit>
        </time>
      </wait>
      <statecheck>
