  }
  else
    testStepFail("Error","Don't get any string array. Please check the section and entry input.");
}

export void prodOperWithSigStatImpOnSpecPwmChk (char operationMode[]) {//operationMode[] gets from key name in section: btnPrsImpOnSpecPwmCfg
