    if ((_pow(10,-6)*(tFallStart - tRiseEnd) <= atol(cfgStrArray[5]) + atol(cfgStrArray[6])) && (_pow(10,-6)*(tFallStart - tRiseEnd) <= atol(cfgStrArray[5]) - atol(cfgStrArray[6])))
