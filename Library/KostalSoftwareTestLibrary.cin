    else
      testStepFail("measure PWM keep time","PWM %s :PWM keep time is %6.2fms,out of range [%s ¡À %sms].",pwmMode,_pow(10,-6)*(tFallStart-tRiseEnd),cfgStrArray[5],cfgStrArray[6]);
  }
}

